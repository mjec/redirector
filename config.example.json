{
	"listen_address": ":8080",
	"default_response": {
		"code": 421,
		"body": "421 Misdirected Request\n\nTarget URI does not match an origin for which the server has been configured.\n",
		"headers": {
			"Connection":   "close",
			"Content-Type": "text/plain"
		},
		"log_hits": true
	},
	"domains": {
		"example.com": {
			"match_subdomains": true,
			"rewrites": [
				{
					"regexp": "^(.*)$",
					"replacement": "https://www.example.com$1",
					"code": 301,
					"log_hits": true
				}
			]
		}
	}
}